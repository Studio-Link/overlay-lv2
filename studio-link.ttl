		lv2:symbol "out0" ;
		lv2:name "Out0"
	] , [

		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "out1" ;
		lv2:name "Out1"
	] .
