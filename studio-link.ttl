<http://studio-link.de/plugins/lv2/studio-link>
        a lv2:Plugin ;
        doap:name "Studio Link" ;
        doap:license <http://opensource.org/licenses/BSD-3-Clause> ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
		a lv2:AudioPort ,
			lv2:InputPort ;
